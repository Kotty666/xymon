#!/bin/sh

# This extension script picks up the $BBTMP/frequenttests.$BBLOCATION
# file generated by bbtest-net. It then re-does the network tests
# with the same parameters.

REDOFILE=$BBTMP/frequenttests.$BBLOCATION
if test -r $REDOFILE
then
	@RUNTIMEDEFS@ $BBHOME/bin/bbtest-net `cat $REDOFILE`
fi

exit 0

